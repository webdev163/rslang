{
  "name": "rslang",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack --mode production --json > stats.json && webpack-bundle-analyzer stats.json build --report report.html --mode static -O && cross-env NODE_ENV=production webpack && serve -s build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "lint": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^10.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "mini-svg-data-uri": "^1.3.3",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "react-refresh": "^0.11.0",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "sass-resources-loader": "^2.2.4",
    "serve": "^13.0.2",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.1",
    "@mui/x-data-grid": "^5.5.1",
    "material-ui-popup-state": "^2.0.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "browserslist": {
    "production": [
      "> 0.25%",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.(ts|tsx)": "npm run lint"
  }
}
